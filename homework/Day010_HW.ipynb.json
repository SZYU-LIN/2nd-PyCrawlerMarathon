    "import requests\n",
    "from bs4 import BeautifulSoup\n",
    "\n",
    "r=requests.get('https://google.com')\n",
    "soup=BeautifulSoup(r.text)\n",
    "soup=soup.find('title')\n",
    "print(r.text)\n",
    "print(type(soup), soup.text)"
